{
    "buildCommand": "npm run build",
    "outputDirectory": "dist/client/browser",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "https://talkingcanvas-backend.onrender.com/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}